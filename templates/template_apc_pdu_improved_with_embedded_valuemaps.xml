<?xml version='1.0' encoding='UTF-8'?>
<zabbix_export version="7.0">
  <templates>
    <template>
      <template>Template APC PDU SNMPv3</template>
      <name>Template APC PDU SNMPv3</name>
      <description>Monitors APC PDU via SNMPv3 using PowerNet-MIB</description>
      <items>
        <item>
          <name>PDU Bank Current</name>
          <type>4</type>
          <snmp_oid>.1.3.6.1.4.1.318.1.1.26.8.3.1.5</snmp_oid>
          <key>pdu.bank.current</key>
          <delay>60s</delay>
          <history>7d</history>
          <trends>365d</trends>
          <status>0</status>
          <value_type>0</value_type>
          <units>A</units>
          <description>Current per bank</description>
        </item>
        <item>
          <name>PDU Bank Load State</name>
          <type>4</type>
          <snmp_oid>.1.3.6.1.4.1.318.1.1.26.8.3.1.4</snmp_oid>
          <key>pdu.bank.loadstate</key>
          <delay>60s</delay>
          <history>7d</history>
          <trends>365d</trends>
          <status>0</status>
          <value_type>4</value_type>
          <units/>
          <description>Load state per bank</description>
        </item>
        <item>
          <name>Device Load State</name>
          <type>4</type>
          <snmp_oid>.1.3.6.1.4.1.318.1.1.26.4.3.1.4</snmp_oid>
          <key>pdu.device.loadstate</key>
          <delay>60s</delay>
          <history>7d</history>
          <trends>365d</trends>
          <status>0</status>
          <value_type>4</value_type>
          <units/>
          <description>Device-wide load state</description>
        </item>
        <item>
          <name>Device Power</name>
          <type>4</type>
          <snmp_oid>.1.3.6.1.4.1.318.1.1.26.4.3.1.5</snmp_oid>
          <key>pdu.device.power</key>
          <delay>60s</delay>
          <history>7d</history>
          <trends>365d</trends>
          <status>0</status>
          <value_type>0</value_type>
          <units>kW</units>
          <description>Device power consumption</description>
        </item>
        <item>
          <name>Device Energy</name>
          <type>4</type>
          <snmp_oid>.1.3.6.1.4.1.318.1.1.26.4.3.1.9</snmp_oid>
          <key>pdu.device.energy</key>
          <delay>60s</delay>
          <history>7d</history>
          <trends>365d</trends>
          <status>0</status>
          <value_type>0</value_type>
          <units>kWh</units>
          <description>Device energy consumed</description>
        </item>
        <item>
          <name>Ambient Temperature</name>
          <type>4</type>
          <snmp_oid>.1.3.6.1.4.1.318.1.1.26.10.2.3.1.3</snmp_oid>
          <key>pdu.sensor.temperature</key>
          <delay>60s</delay>
          <history>7d</history>
          <trends>365d</trends>
          <status>0</status>
          <value_type>0</value_type>
          <units>C</units>
          <description>Ambient temperature</description>
        </item>
        <item>
          <name>Relative Humidity</name>
          <type>4</type>
          <snmp_oid>.1.3.6.1.4.1.318.1.1.26.10.2.3.1.4</snmp_oid>
          <key>pdu.sensor.humidity</key>
          <delay>60s</delay>
          <history>7d</history>
          <trends>365d</trends>
          <status>0</status>
          <value_type>0</value_type>
          <units>%</units>
          <description>Relative humidity</description>
        </item>
      </items>
    <macros><macro><macro>{$TEMP_HIGH}</macro><value>35</value></macro><macro><macro>{$HUMIDITY_HIGH}</macro><value>70</value></macro><macro><macro>{$LOAD_THRESHOLD}</macro><value>80</value></macro></macros><graphs><graph><name>PDU Current &amp; Power</name><width>900</width><height>200</height><yaxismin>0.0000</yaxismin><yaxismax>100.0000</yaxismax><graph_items><graph_item><drawtype>0</drawtype><sortorder>0</sortorder><color>00AA00</color><item><host>Template APC PDU SNMPv3</host><key>pdu.bank.current</key></item></graph_item></graph_items><graph_items><graph_item><drawtype>0</drawtype><sortorder>0</sortorder><color>0000AA</color><item><host>Template APC PDU SNMPv3</host><key>pdu.device.power</key></item></graph_item></graph_items></graph></graphs><value_maps><value_map><name>Load State Map</name><mapping><value>1</value><newvalue>Low Load</newvalue></mapping><mapping><value>2</value><newvalue>Normal</newvalue></mapping><mapping><value>3</value><newvalue>Near Overload</newvalue></mapping><mapping><value>4</value><newvalue>Overload</newvalue></mapping></value_map></value_maps></template>
  </templates>
<value_maps><value_map><name>Load State Map</name><mapping><value>1</value><newvalue>Low Load</newvalue></mapping><mapping><value>2</value><newvalue>Normal</newvalue></mapping><mapping><value>3</value><newvalue>Near Overload</newvalue></mapping><mapping><value>4</value><newvalue>Overload</newvalue></mapping></value_map></value_maps></zabbix_export>
